{
  "name": "k8s-pod-network",
  "cniVersion": "0.3.0",
  "plugins": [
    {
      "type": "calico",
      "etcd_endpoints": "http://10.110.0.136:6666",
      "log_level": "info",
      "mtu": 1500,
      "ipam": {
        "type": "calico-ipam"
      },
      "policy": {
        "type": "k8s"
      },
      "kubernetes": {
        "kubeconfig": "/etc/cni/net.d/calico-kubeconfig"
      }
    },
    {
      "type": "portmap",
      "snat": true,
      "capabilities": {
        "portMappings": true
      }
    },
    {
      "name": "linkerd2-cni",
      "type": "linkerd2-cni",
      "log_level": "warn",
      "policy": {
        "type": "k8s",
        "k8s_api_root": "https://10.110.0.1:443",
        "k8s_auth_token": "MyAwesomeToken"
      },
      "kubernetes": {
        "kubeconfig": "/etc/cni/net.d/ZZZ-linkerd2-cni-kubeconfig"
      },
      "linkerd2": {
        "incoming-proxy-port": -1,
        "outgoing-proxy-port": -1,
        "proxy-uid": -1,
        "ports-to-redirect": [],
        "inbound-ports-to-ignore": [],
        "outbound-ports-to-ignore": [],
        "simulate": false
      }
    }
  ]
}
